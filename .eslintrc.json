{
  "extends": "airbnb-base",
  "env": {
    "jest": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": ["**/*.spec.js"]
    }]
  }
}
